{
	"extends": ["next/core-web-vitals", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"root": true,
	"rules": {
		"import/no-unresolved": "error",
		"semi": ["error", "always"],
		"quotes": ["error", "double"]
	}
}
